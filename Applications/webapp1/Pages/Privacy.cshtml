@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<div>
    <h2>環境変数</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (string key in Environment.GetEnvironmentVariables().Keys.OfType<string>().OrderBy(k => k))
            {
            <tr>
                <td><span>@key</span></td>
                <td><span>@(Environment.GetEnvironmentVariable(key))</span></td>
            </tr>
            }
        </tbody>
    </table>
</div>

<div>
    <h2>Configuration</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var kvp in Model.configDict)
            {
            <tr>
                <td><span>@(kvp.Key)</span></td>
                <td><span>@(kvp.Value)</span></td>
            </tr>
            }
        </tbody>
    </table>
</div>